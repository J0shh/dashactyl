{
    "index": "index.ejs",
    "notfound": "404.ejs",
    "redirect": {
        "callback": "/dashboard",
        "logout": "/",
        "deleteserver": "/servers",
        "updateservers": "/dashboard",
        "createserver": "/servers",
        "failedcreateserver": "/servers/new",
        "modifyserver": "/servers",
        "failedmodifyserver": "/servers/edit"
    },
    "pages": {
        "dashboard": "dashboard.ejs",
        "servers": "servers.ejs",
        "servers/new": "create.ejs",
        "servers/edit": "edit.ejs"
    },
    "mustbeloggedin": [
        "/dashboard",
        "/servers",
        "/servers/new",
        "/servers/edit"
    ],
    "variables": {
        "faviconlink": "",
        "home": {
            "title": "Dashactyl - Home",
            "name": "Dashactyl",
            "topmsg": "Dashactyl",
            "bottommsg": "An dashboard addition to the Pterodactyl Panel.",
            "buttontext": {
                "loggedin": {
                    "text": "Dashboard",
                    "link": "/dashboard"
                },
                "loggedout": {
                    "text": "Login",
                    "link": "/login"
                }
            }
        },
        "dashboard": {
            "title": "Dashactyl - Dashboard"
        },
        "servers": {
            "title": {
                "list": "Dashactyl - Servers",
                "create": "Dashactyl - Create Server",
                "modify": "Dashactyl - Modify Server"
            }
        }
    }
}