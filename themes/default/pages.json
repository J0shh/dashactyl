{
    "index": "index.ejs",
    "notfound": "404.ejs",
    "assets": "assets",
    "callbackredirect": "/dashboard",
    "logoutredirect": "/",
    "pages": {
        "dashboard": "dashboard.ejs",
        "servers": "servers.ejs"
    },
    "mustbeloggedin": [
        "/dashboard",
        "/servers"
    ],
    "variables": {
        "home": {
            "title": "Dashactyl - Home",
